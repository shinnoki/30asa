<h2>Recent Updates</h2>
<table>
  <tr>
    <td>User</td>
    <td>Song</td>
    <td>Score</td>
    <td>Updated at</td>
<% Record.find(:all, :order=>'created_at desc', :limit=>20).each do |record| %>
  <tr>
    <td><%= link_to record.user.name, record.user %></td>
    <td><%= link_to record.song.title, record.song %></td>
    <td><%= record.score %></td>
    <td><%= record.created_at %></td>
    <td><%= link_to 'Edit', edit_record_path(record) %></td>
    <td><%= link_to 'Destroy', record, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
